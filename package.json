{
  "name": "@phanect/eslint-plugin",
  "version": "2022.1.23",
  "description": "ESLint configuration for Jumpei Ogawa (@phanect)'s private projects",
  "main": "lib/config.js",
  "scripts": {
    "build": "node --trace-warnings --unhandled-rejections=strict ./scripts/builder.js",
    "test": "jest",
    "lint": "eslint --ignore-pattern tests/ --ext=.js,.jsx,.ts,.tsx,.vue .",
    "clean": "git clean -dX --force",
    "release": "npm run build && npm publish . --access public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phanect/eslint-plugin.git"
  },
  "author": "Jumpei Ogawa",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=10.x"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint-plugin-document-write": "latest",
    "eslint-plugin-editorconfig": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-nuxt": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-vue": "latest",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.5"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  }
}
